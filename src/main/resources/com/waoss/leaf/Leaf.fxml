<?import com.waoss.leaf.syntax.JavaSyntaxArea?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" fx:id="anchorPane" prefHeight="598.0" prefWidth="1001.0"
            xmlns="http://javafx.com/javafx/8.0.112" fx:controller="com.waoss.leaf.LeafController">
    <children>
        <MenuBar prefHeight="29.0" prefWidth="1001.0">
            <menus>
                <Menu mnemonicParsing="false" text="File">
                    <items>
                        <MenuItem fx:id="fileNew" mnemonicParsing="false" onAction="#fileNewOnAction" text="New"/>
                        <MenuItem fx:id="fileOpen" mnemonicParsing="false" onAction="#fileOpenOnAction" text="Open"/>
                        <MenuItem fx:id="fileClose" mnemonicParsing="false" onAction="#fileCloseOnAction" text="Close"/>
                    </items>
                </Menu>
                <Menu mnemonicParsing="false" text="Edit">
                    <items>
                        <MenuItem mnemonicParsing="false" text="Delete"/>
                    </items>
                </Menu>
                <Menu mnemonicParsing="false" text="Help">
                    <items>
                        <MenuItem mnemonicParsing="false" text="About"/>
                    </items>
                </Menu>
            </menus>
        </MenuBar>
        <JavaSyntaxArea fx:id="codeArea" layoutY="29.0" prefHeight="569.0" prefWidth="1001.0"/>
    </children>
</AnchorPane>
